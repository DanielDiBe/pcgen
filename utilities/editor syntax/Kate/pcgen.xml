			<itemData name="Sub-Tags" defStyleNum="dsDataType" />
			<itemData name="Assignment" defStyleNum="dsString" />
			<itemData name="Pipes" defStyleNum="dsDecVal" />
			<itemData name="Numbers" defStyleNum="dsOthers" />
			<itemData name="Comment" defStyleNum="dsComment" />
			<itemData name="DescText" defStyleNum="dsComment" bold="1" />
			<itemData name="Variables" defStyleNum="dsDataType" italic="1" />
			<itemData name="ASPECT/VAR Defined" defStyleNum="dsKeyword" italic="1" />
			<itemData name="Altered" defStyleNum="dsError" />
			<itemData name="Keywords" defStyleNum="dsFunction" />
		</itemDatas>
