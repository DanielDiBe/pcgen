		<contexts>
			<context attribute="Normal Text" lineEndContext="Normal" name="Normal" >
				<IncludeRules context="KeepNeat" />
				<keyword attribute="Tags" String="tags" />
				<keyword attribute="Tags" String="colortext" context="Descriptive" />
				<keyword attribute="Altered" String="modifiers" />
				<RegExpr attribute="Comment" String="^#.*$" />
				<StringDetect String="ASPECT" attribute="Tags" context="DefineAspect" />
				<StringDetect String="DEFINE" attribute="Tags" context="DefineVar" />
				<StringDetect String="ABILITY" attribute="Tags" context="DefineAbility" />
				<keyword attribute="Tags" String="TagsWithSub" context="Tags2Sub" />
				<DetectChar char="|" attribute="Pipes" />
				<RegExpr attribute="PRETags" String="[!]*PRE[\w]*" context="PRExxx" />
			</context>
			<context name="KeepNeat" >
				<RegExpr String="[]:=,.()[]" attribute="Normal Text" />
				<RegExpr attribute="PRETags" String="[!]*PRE[\w]*" context="PRExxx" />
				<keyword String="typers" attribute="Tags" context="Typing"/>
				<RegExpr attribute="Normal Text" context="Normal" String="[\t]" />
				<keyword attribute="Altered" String="modifiers" />
			</context>
			<context name="Descriptive" attribute="DescText" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Variables" />
				<RegExpr String="[%][\d]*" attribute="Sub-Tags" />
			</context>
			<context name="DefineAspect" attribute="ASPECT/VAR Defined" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Descriptive" />
			</context>
			<context name="DefineVar" attribute="ASPECT/VAR Defined" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Variables" />
			</context>
			<context name="Variables" attribute="Variables" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" />
				<Int attribute="Numbers" />
				<keyword attribute="Numbers" String="statmods" />
			</context>
			<context name="PRExxx" attribute="Normal Text" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<Int attribute="Numbers" context="Normal" />
				<RegExpr String="[\w]" context="Normal" />
			</context>
			<context name="Tags2Sub" attribute="Sub-Tags" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Clarify" />
			</context>
			<context name="Clarify" attribute="Assignment" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<DetectChar char="|" attribute="Pipes" context="Variables" />
			</context>
			<context name="Typing" attribute="Keywords" lineEndContext="Normal" >
				<IncludeRules context="KeepNeat" />
				<keyword attribute="Keywords" String="words" />
				<DetectChar char="|" attribute="Pipes" context="#pop" />
			</context>
			<context name="DefineAbility" lineEndContext="Normal" >
				<DetectChar char=":" attribute="Normal Text" context="Tags2Sub" />
				<DetectChar char="|" attribute="Pipes" context="Tags2Sub" />
			</context>
		</contexts>
		<itemDatas>
			<itemData name="Normal Text" defStyleNum="dsNormal" />
			<itemData name="Tags" defStyleNum="dsKeyword" />
